#include <stdio.h>
#include <cs50.h>

int main() {
  int n = 0;
  int c = 0;
  int i = 0;
  printf("------PRIME NUMBER CHECKER------\n\n-Developed by hman-08\n\n\nEnter any number:");
  scanf("%d", &n);

  //logic
  for (i = 1; i <= n; i++) {
      if (n % i == 0) {
         c++;
      }
  }

  if (c == 2) {
        printf("Your number is Prime\n");
  }
  else {
        printf("Your number is not prime\n");
  }
  return 0;
}
